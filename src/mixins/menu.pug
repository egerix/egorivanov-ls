- var menuLinks = ["Обо мне", "Навыки", "Работы", "Отзывы"];

mixin menu(mod)
    -var menuClass = mod === undefined ? "menu" : `menu menu--${mod}`
    nav(class=menuClass)
        ul.menu__list
            each item in menuLinks
                li.menu__item
                    a(href="#").menu__link #{item}